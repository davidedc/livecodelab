{
  "name": "livecodelab",
  "version": "0.1.0",
  "description": "A live coding environment for the browser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidedc/livecodelab/issues"
  },
  "repository": "davidedc/livecodelab",
  "scripts": {
    "start": "npm run start:lcl",
    "start:lcl": "webpack-dev-server --mode development",
    "start:site": "npx @11ty/eleventy --serve --port 8081",
    "start:wiki": "tiddlywiki docs/wiki --server",
    "build": "npm run build:lcl && npm run build:site && npm run build:wiki",
    "build:lcl": "webpack --mode production",
    "build:site": "npx @11ty/eleventy",
    "build:wiki": "tiddlywiki docs/wiki --output ./dist/wiki --build index",
    "docs": "docco docs/**/*.md -o dist/docs",
    "docs:src": "docco src/coffee/**/*.coffee -o dist/docs/src",
    "prettier": "prettier --single-quote",
    "test": "mochapack --webpack-config webpack.config.js \"tests/**/*.js\"",
    "testone": "mochapack --webpack-config webpack.config.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.10.0",
    "@babel/core": "7.7.7",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.7",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "coffee-loader": "0.9.0",
    "coffeescript": "2.4.1",
    "css-loader": "3.4.0",
    "decaffeinate": "^6.0.9",
    "dentist": "1.0.3",
    "docco": "0.8.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "file-loader": "5.0.2",
    "mocha": "6.2.2",
    "mochapack": "^1.1.13",
    "pegjs": "0.10.0",
    "pegjs-loader": "0.5.5",
    "prettier": "1.19.1",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.1",
    "tiddlywiki": "5.1.21",
    "url-loader": "3.0.0",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "buzz": "1.2.1",
    "underscore": "1.9.1"
  }
}
