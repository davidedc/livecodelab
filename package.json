{
  "name": "livecodelab",
  "version": "0.1.0",
  "description": "A live coding environment for the browser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidedc/livecodelab/issues"
  },
  "repository": "davidedc/livecodelab",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "docs": "docco docs/**/*.md -o dist/docs",
    "docs:src": "docco src/coffee/**/*.coffee -o dist/docs/src",
    "test": "mocha-webpack --webpack-config webpack.config.js \"tests/**/*.js\"",
    "testone": "mocha-webpack --webpack-config webpack.config.js",
    "prettier": "prettier --single-quote",
    "servewiki": "tiddlywiki docs/wiki --server",
    "buildwiki": "tiddlywiki docs/wiki --output ./dist/wiki --build index"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "coffee-loader": "0.9.0",
    "coffeescript": "2.3.2",
    "css-loader": "1.0.0",
    "dentist": "1.0.3",
    "docco": "0.8.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-prettier": "2.4.0",
    "file-loader": "2.0.0",
    "loglevel": "1.4.1",
    "mocha": "5.2.0",
    "mocha-webpack": "1.1.0",
    "pegjs": "0.10.0",
    "pegjs-loader": "0.5.4",
    "prettier": "1.10.2",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.0",
    "tiddlywiki": "5.1.14",
    "underscore": "1.8.3",
    "url-loader": "1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.9"
  }
}
