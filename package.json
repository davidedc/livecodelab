{
  "name": "livecodelab",
  "version": "0.1.0",
  "description": "A live coding environment for the browser",
  "scripts": {
    "build": "webpack -p",
    "devbuild": "webpack -d",
    "serve": "webpack-dev-server -d --content-base dist/ --inline",
    "docs": "docco docs/**/*.md -o dist/docs",
    "docs:src": "docco src/coffee/**/*.coffee -o dist/docs/src",
    "pretest": "pegjs ./src/grammar/lcl.pegjs ./src/generated/parser.js",
    "test": "nodeunit tests/langtests"
  },
  "devDependencies": {
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "css-loader": "^0.25.0",
    "dentist": "1.0.3",
    "docco": "^0.7.0",
    "file-loader": "^0.9.0",
    "nodeunit": "^0.9.1",
    "pegjs": "^0.9.0",
    "pegjs-loader": "^0.4.0",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
